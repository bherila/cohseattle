
@model Dinamico.Models.ContentPart
@{ 
    Content.Define(ie =>
        {
            ie.Title = "Image";
            ie.Image("Image");
            ie.Enum("Float", typeof(MWMvc.Float)).DefaultValue(MWMvc.Float.Middle);
            ie.FreeText("Caption");
            ie.Text("AltText").DefaultValue("");
        }); 
}